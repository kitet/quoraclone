<div class='page-header'>  
    <h1>Sign Up</h1>
</div> 
<form class='form-group'>
{{input
    type='text'
    class='form-control'
    value=email
    placeholder='email@email.com'
}}
{{input
    type='password'
    class='form-control'
    value=password
    placeholder='password'
}}
<button  
    class='btn btn-default'
    {{action 'signUp'}}
>Sign Up
</button> 
</form>
{{outlet}}
